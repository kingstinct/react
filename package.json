{
  "name": "@kingstinct/react",
  "version": "1.0.2",
  "description": "Kingstinct Hooks Utility Library",
  "author": "Robert Herber",
  "license": "MIT",
  "files": [
    "components",
    "contexts",
    "hooks",
    "primitives",
    "utils",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "start": "cp index.expo.js index.js && expo start",
    "android": "cp index.expo.js index.js && expo run:android",
    "ios": "cp index.expo.js index.js && expo run:ios",
    "web": "cp index.expo.js index.js && expo start --web",
    "build": " rimraf utils primitives hooks contexts components stories && tsc --declaration -p ./tsconfig.build.json",
    "test": "concurrently \"yarn test-only\" \"yarn lint\" \"tsc --noEmit\"",
    "test-only": "jest",
    "lint": "eslint . --cache",
    "prepare": "husky install",
    "prepublishOnly": "concurrently \"yarn build\" \"yarn test\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "yarn test && npx release-it"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": ">=7",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@expo/webpack-config": "^0.17.2",
    "@graphql-eslint/eslint-plugin": ">=3",
    "@react-native-async-storage/async-storage": "^1.17.7",
    "@react-native-community/netinfo": "^9.3.0",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@release-it/conventional-changelog": "^5.1.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^28.1.4",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^18.7.8",
    "@types/react": "18",
    "@types/react-native": "0.69",
    "@typescript-eslint/eslint-plugin": ">=5",
    "@typescript-eslint/parser": ">=5",
    "babel-loader": "^8.2.5",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^7.3.0",
    "eslint": ">=8",
    "eslint-config-airbnb": ">=19",
    "eslint-config-airbnb-base": ">=15",
    "eslint-config-kingstinct": "^5.0.23",
    "eslint-import-resolver-typescript": ">=3",
    "eslint-plugin-comment-length": "^0.6.2",
    "eslint-plugin-functional": ">=4",
    "eslint-plugin-import": ">=2",
    "eslint-plugin-jest": ">=26",
    "eslint-plugin-jsonc": ">=2",
    "eslint-plugin-jsx-a11y": ">=6",
    "eslint-plugin-react": ">=7",
    "eslint-plugin-react-hooks": ">=4",
    "eslint-plugin-react-native": ">=4",
    "eslint-plugin-react-native-a11y": ">=3",
    "eslint-plugin-sort-keys-fix": ">=1",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-yml": ">=1",
    "expo": ">=45",
    "expo-application": "^4.1.0",
    "expo-device": "^4.2.0",
    "expo-haptics": "^11.2.0",
    "expo-status-bar": "^1.4.0",
    "expo-system-ui": "^1.2.0",
    "graphql": ">=16",
    "husky": "^8.0.1",
    "jest": "27",
    "jest-expo": "^45.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "react": "17",
    "react-dom": "17",
    "react-native": "0.68",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-paper": "^4.12.4",
    "react-native-reanimated": "2.6.0",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-web": "^0.18.7",
    "react-test-renderer": "17",
    "release-it": "^15.3.0",
    "require-from-string": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "27",
    "typescript": ">=4"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {
    "@types/react": ">=16",
    "react": ">=16",
    "react-native-reanimated": ">=2"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "nanoid": "^4.0.0",
    "ts-pattern": "^4.0.5",
    "urql": ">=2",
    "zustand": "^4.3.6"
  }
}
