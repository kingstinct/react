{
  "name": "@kingstinct/react",
  "version": "1.0.4",
  "description": "Kingstinct Hooks Utility Library",
  "author": "Robert Herber",
  "license": "MIT",
  "files": [
    "components",
    "contexts",
    "hooks",
    "primitives",
    "utils",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "start": "cp index.expo.js index.js && expo start",
    "android": "cp index.expo.js index.js && expo run:android",
    "ios": "cp index.expo.js index.js && expo run:ios",
    "web": "cp index.expo.js index.js && expo start --web",
    "build": " rimraf utils primitives hooks contexts components stories && tsc --declaration -p ./tsconfig.build.json",
    "test": "concurrently \"yarn test-only\" \"yarn lint\" \"yarn typecheck\"",
    "typecheck": "tsc --noEmit",
    "test-only": "jest",
    "lint": "eslint . --cache",
    "prepare": "husky install",
    "prepublishOnly": "concurrently \"yarn build\" \"yarn test\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "yarn test && npx release-it"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.js": "eslint --fix",
    "*.json": "eslint --fix",
    "*.graphql": "eslint --fix",
    "*.yml": "eslint --fix"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.22.9",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^18.1.1",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@release-it/conventional-changelog": "^7.0.0",
    "@storybook/addon-actions": "^7.1.0",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-native": "~0.72.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "babel-loader": "^9.1.3",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-kingstinct": "^5.1.1",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-comment-length": "^1.4.4",
    "eslint-plugin-functional": "^5.0.8",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-react-native-a11y": "^3.3.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^48.0.0",
    "eslint-plugin-yml": "^1.8.0",
    "expo": "^49.0.3",
    "expo-application": "^5.3.0",
    "expo-device": "^5.4.0",
    "expo-haptics": "^12.4.0",
    "expo-status-bar": "^1.6.0",
    "expo-system-ui": "^2.4.0",
    "graphql": "^16.7.1",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest-expo": "^49.0.0",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^13.2.3",
    "lodash": "^4.17.21",
    "metro-config": "^0.77.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "~0.72.3",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-paper": "^5.9.1",
    "react-native-reanimated": "3.3.0",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.22.1",
    "react-native-web": "^0.19.7",
    "react-test-renderer": "^18.2.0",
    "release-it": "^16.1.3",
    "require-from-string": "^2.0.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "peerDependencies": {
    "@types/react": ">=16",
    "react": ">=16",
    "react-native-reanimated": ">=2"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "ts-pattern": "^5.0.4",
    "urql": "^4.0.4",
    "zustand": "^4.3.9"
  }
}
